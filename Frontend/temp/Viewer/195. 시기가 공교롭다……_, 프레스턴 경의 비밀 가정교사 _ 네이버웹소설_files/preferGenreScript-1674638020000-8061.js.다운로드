var PreferGenre=jindo.$Class({_TOOLTIP_COOKIE_NAME:"novel_pgt",_preferGenreTypeCode:null,$init:function(){this.initPreferGenreTypeCode();this._setEvent();this.openPreferGenreTooltip();this.changeUserPreferGenre();},initPreferGenreTypeCode:function(){if(this._preferGenreTypeCode==null){var a=$("preferGenreType");if(a.value==""){this._preferGenreTypeCode="000";}else{this._preferGenreTypeCode=a.value;}}},_setEvent:function(){this._oBubbler=new jindo.Bubbler(document.body);this._oBubbler.attach({"_closePreferGenreTooltip:click":$Fn(this._closePreferGenreTooltip,this).bind(),"_openPreferGenreSettingLayer:click":$Fn(this._openPreferGenreSettingLayer,this).bind(),"_closePreferGenreSettingLayer:click":$Fn(this._closePreferGenreSettingLayer,this).bind(),"_openPreferGenreSettingFooterLayer:click":$Fn(this._openPreferGenreSettingFooterLayer,this).bind(),"_closePreferGenreSettingFooterLayer:click":$Fn(this._closePreferGenreSettingFooterLayer,this).bind(),"_closePreferGenreSettingAllLayer:click":$Fn(this._closePreferGenreSettingAllLayer,this).bind(),"_selectPreferGenre:click":$Fn(this._selectPreferGenre,this).bind(),"_initPreferGenre:click":$Fn(this._initPreferGenre,this).bind(),"_savePreferGenreSetting:click":$Fn(this._savePreferGenreSetting,this).bind()});},add:function(b,a){jindo.$Cookie().set(b,a,365,"novel.naver.com");},remove:function(a){jindo.$Cookie().remove(a,"novel.naver.com");},openPreferGenreTooltip:function(){var a=jindo.$Cookie().get(this._TOOLTIP_COOKIE_NAME);if(a!="Y"&&$Element("preferGenreTooltip")!=null){$Element("preferGenreTooltip").show();}},_closePreferGenreTooltip:function(b,a,c){b.stopDefault();jindo.$Cookie().set(this._TOOLTIP_COOKIE_NAME,"Y",1000,"novel.naver.com");$Element("preferGenreTooltip").hide();clickcr(this,"prf.close","",b);},_openPreferGenreSettingLayer:function(b,a,c){b.stopDefault();if($Element("preferGenreSettingLayer").css("display")=="none"){this._closePreferGenreSettingFooterLayer();$Element("preferGenreSettingLayer").show();$Element("preferGenreSettingBtn").addClass("on");clickcr(a,"prf.info","","",b);}else{this._closePreferGenreSettingLayer(b,a,c);}},_closePreferGenreSettingLayer:function(b,a,c){if(b!=null){b.stopDefault();clickcr(a,"prf.cls","",b);}$Element("preferGenreSettingLayer").hide();$Element("preferGenreSettingBtn").removeClass("on");this.cancelSelectPreferGenre();},_openPreferGenreSettingFooterLayer:function(b,a,c){b.stopDefault();if($Element("preferGenreSettingFooterLayer").css("display")=="none"){this._closePreferGenreSettingLayer();$Element("preferGenreSettingFooterLayer").show();$Element("preferGenreSettingFooterBtn").addClass("on");clickcr(a,"prf.info","","",b);}else{this._closePreferGenreSettingFooterLayer(b,a,c);}},_closePreferGenreSettingFooterLayer:function(b,a,c){if(b!=null){b.stopDefault();clickcr(a,"prf.cls","",b);}$Element("preferGenreSettingFooterLayer").hide();$Element("preferGenreSettingFooterBtn").removeClass("on");this.cancelSelectPreferGenre();},_closePreferGenreSettingAllLayer:function(b,a,c){this._closePreferGenreSettingLayer();this._closePreferGenreSettingFooterLayer();},cancelSelectPreferGenre:function(){var a=$("preferGenreType");var b="000";if(a.value!=""){b=a.value;}this.changePreferGenre(b);},_selectPreferGenre:function(b,a,c){var d=c[0];if(this._preferGenreTypeCode!=d){this.changePreferGenre(d);}},changePreferGenre:function(a){jQuery("input:radio[name='sl_genre']:radio[value="+this._preferGenreTypeCode+"]").prop("checked",false);jQuery("input:radio[name='sl_genre']:radio[value="+a+"]").prop("checked",true);jQuery("input:radio[name='fot_sl_genre']:radio[value="+this._preferGenreTypeCode+"]").prop("checked",false);jQuery("input:radio[name='fot_sl_genre']:radio[value="+a+"]").prop("checked",true);$Element("li_preferGenre"+this._preferGenreTypeCode).removeClass("on");$Element("li_preferGenre"+a).addClass("on");$Element("fot_li_preferGenre"+this._preferGenreTypeCode).removeClass("on");$Element("fot_li_preferGenre"+a).addClass("on");this._preferGenreTypeCode=a;},_initPreferGenre:function(b,a,c){b.stopDefault();clickcr(a,"prf.ini","",b);this.changePreferGenre("000");this.showPreferGenreSettingChangedToast("init",c[0]);},_savePreferGenreSetting:function(c,b,d){c.stopDefault();clickcr(b,"prf.sav","",c);var e=jQuery('input:radio[name="sl_genre"]:checked').val();if(d[0]=="footer"){e=jQuery('input:radio[name="fot_sl_genre"]:checked').val();}var a=$("preferGenreType");if(a.value==""||a.value!=e){this.add($("encryptedIdNo").value,e);if(e=="101"){clickcr(b,"prf.rom","",c);}else{if(e=="102"){clickcr(b,"prf.fan","",c);}else{if(e=="103"){clickcr(b,"prf.hro","",c);}else{if(e=="109"){clickcr(b,"prf.rof","",c);}else{if(e=="110"){clickcr(b,"prf.mof","",c);}else{if(e=="000"){clickcr(b,"prf.non","",c);}}}}}}window.location.reload();}else{$Element("preferGenreSettingLayer").hide();$Element("preferGenreSettingBtn").removeClass("on");$Element("preferGenreSettingFooterLayer").hide();$Element("preferGenreSettingFooterBtn").removeClass("on");}},changeUserPreferGenre:function(){var a=$("encryptedIdNo").value;var b=$("preferGenreType").value;var c=$("preferGenreTypeChange").value;if(c=="true"){this.setPreferGenre(a,b);window.location.reload();}},setPreferGenre:function(a,b){this.add(a,b);this.remove("default");},showPreferGenreSettingChangedToast:function(a,d){var b="fot_";var c="preferGenreDim";if(d=="footer"){c=b+c;}jQuery("#"+c).show();setTimeout(function(){jQuery("#"+c).hide();},1000);}});